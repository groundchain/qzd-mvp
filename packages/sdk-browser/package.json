{
  "name": "@qzd/sdk-browser",
  "version": "0.0.0",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": ["dist"],
  "scripts": {
    "build": "tsup src/index.ts --dts --format esm --out-dir dist --clean",
    "test": "vitest run",
    "test:contract": "vitest run tests/contract.test.ts",
    "lint": "eslint \"src/**/*.ts\"",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@types/node": "^20.11.0",
    "tsup": "^8.5.0",
    "typescript": "^5.3.3",
    "vitest": "^1.3.1"
  }
}
