{
  "name": "@qzd/shared",
  "version": "0.2.0",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./request-security": {
      "import": "./dist/request-security.js",
      "types": "./dist/request-security.d.ts"
    },
    "./ui/theme": {
      "import": "./dist/ui/theme.js",
      "types": "./dist/ui/theme.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts src/request-security.ts src/ui/theme.ts --dts --format esm --out-dir dist --clean",
    "test": "vitest run",
    "lint": "eslint \"src/**/*.ts\"",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@noble/curves": "^1.6.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@vitest/coverage-v8": "^1.3.1",
    "@types/node": "^20.11.0",
    "typescript": "^5.3.3",
    "vitest": "^1.3.1"
  }
}
