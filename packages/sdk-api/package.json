{
  "name": "@qzd/sdk-api",
  "version": "0.1.2",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./src/index.ts"
    },
    "./browser": {
      "import": "./dist/browser/index.js",
      "types": "./src/browser/index.ts"
    },
    "./browser/*": {
      "import": "./dist/browser/*.js",
      "types": "./src/browser/*.ts"
    },
    "./node": {
      "import": "./dist/node/index.js",
      "types": "./src/node/index.ts"
    },
    "./node/*": {
      "import": "./dist/node/*.js",
      "types": "./src/node/*.ts"
    },
    "./server": {
      "import": "./dist/server/index.js",
      "types": "./src/server/index.ts"
    },
    "./server/*": {
      "import": "./dist/server/*.js",
      "types": "./src/server/*.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "generate": "pnpm --dir ../../ run gen:sdks",
    "prebuild": "pnpm run generate",
    "build": "tsc -p tsconfig.build.json",
    "lint": "eslint --no-warn-ignored \"src/**/*.ts\" --ignore-pattern \"src/**/generated/**\"",
    "typecheck": "node ../../scripts/ensure-sdk-api-generated.mjs && tsc --noEmit"
  },
  "peerDependencies": {
    "@nestjs/common": "^10.0.0",
    "axios": "^1.7.7",
    "rxjs": "^7.8.1"
  },
  "devDependencies": {
    "@nestjs/common": "^10.0.0",
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.0",
    "axios": "^1.7.7",
    "rxjs": "^7.8.1",
    "typescript": "^5.3.3"
  }
}
