{
  "name": "qzd-mvp",
  "private": true,
  "version": "0.0.0",
  "packageManager": "pnpm@8.15.4",
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "build": "pnpm -r build",
    "test": "pnpm -r test",
    "lint": "pnpm -r lint",
    "typecheck": "pnpm -r typecheck",
    "prepare": "husky",
    "oas:lint": "redocly lint openapi/openapi.yaml",
    "oas:mock": "start-server-and-test \"prism mock openapi/openapi.yaml --host 127.0.0.1 --port 4010\" http-get://127.0.0.1:4010/health/live \"node ./scripts/oas-mock-check.mjs\"",
    "oas:docs": "redocly build-docs openapi/openapi.yaml --output openapi/dist/index.html"
  },
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@redocly/cli": "^2.2.1",
    "@stoplight/prism-cli": "^5.14.2",
    "ajv": "^8",
    "eslint": "^9.36.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.21",
    "husky": "^9.1.7",
    "prettier": "^3.6.2",
    "start-server-and-test": "^2.1.2",
    "tsup": "^8.5.0",
    "typescript-eslint": "^8.44.1"
  }
}
